function detar(){
  tar -xvzf "$1"
  rm "$1"
  cd $(echo "$1"| cut -d'.' -f 1)
}

function retar(){
  tar -cvzf "$1.tgz" "$1"
}

function fix_rosbag(){
  new_bag=$(echo "$1"| cut -d'.' -f 1)".bag"
  echo "Moving $1 -> $new_bag"
  mv $1 $new_bag 
  echo "Reindexing bag: $new_bag..."
  rosbag reindex "$new_bag"
  #echo "Fixing bag..."
  #rosbag fix "$1" $(echo "$1"| cut -d'.' -f 1)".bag"
  echo "Removing currupt bag files..."
  #rm "$1"
  rm $(echo "$1"| cut -d'.' -f 1)".orig.bag"
}

