extract-all () 
{ 
  NO_FLIGHT_DIR="noFlight";
  if [[ -d "${NO_FLIGHT_DIR}" ]]; then
      echo "grabbing ${NO_FLIGHT_DIR} logs";
      mv -v "${NO_FLIGHT_DIR}"/* .;
      rm -rv "${NO_FLIGHT_DIR}";
  fi;
  echo "current dir: $(pwd)";
  mkdir -p archive;
  for x in *.tgz;
  do
      echo "  $x" && tar -xzf $x && mv $x archive;
  done;
  echo ",extract-all done!";
  ls --color=auto
}


