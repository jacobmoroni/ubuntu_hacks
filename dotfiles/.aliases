function git_branch_hash() {
  hash=$(git rev-parse HEAD)
  branch=$(git branch -a --contains=$hash | grep "remotes" | grep -vwE "(HEAD)")
  echo "$branch"
  if [[ -z "${branch}" ]]; then
    echo $(git branch -a --contains=$hash) 
  fi
}

